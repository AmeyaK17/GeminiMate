{
    "builds": [
      {
        "src": "backend/src/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "/build"
        }
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/backend/src/index.js"
      },
      {
        "source": "/(.*)",
        "destination": "/"
      }
    ]
  }
  